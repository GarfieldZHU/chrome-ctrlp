{
    "name": "Tab Closer",
    "version": "1.0",
    "description": "Close tabs in different ways.",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
      "default_title": "Shimarin",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }